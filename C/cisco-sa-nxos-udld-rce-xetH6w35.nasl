include('cisco_workarounds.inc');
include('ccf.inc');

var product_info = cisco::get_product_info(name:'Cisco NX-OS Software');

var cbi = '';
smus = make_array();

# MDS 9000 Series
if ('MDS' >< product_info.device && product_info.model =~ "^9[0-9]{3}")
{
    cbi = 'CSCvv96088, CSCvw38981';
}
else if ('Nexus' >< product_info.device)
{
  if (product_info.model =~ "^9[0-9]{3}")
  {
    # OS detection sets this kb if in ACI mode
    var is_aci_mode = get_kb_item("Host/Cisco/apic/env");
    if (!empty_or_null(is_aci_mode))
      audit(AUDIT_HOST_NOT, 'affected');
    else
    {
      cbi = 'CSCvv78238, CSCvw38964';
      smus['7.0(3)I7(8)'] = 'CSCvv78238';
      smus['7.0(3)I7(9)'] = 'CSCvv78238';
    }
  }
  else if (product_info.model =~ "^7[0-9]{3}")
  {
    cbi = 'CSCvv96088, CSCvw38981';
    smus['8.2(6)'] = 'CSCvx15395';
  }
  else if (product_info.model =~ "^6[0-9]{3}")
  {
    cbi = 'CSCvv96090, CSCvw38982';
  }
  else if (product_info.model =~ "^5[56][0-9]{2}")
  {
    cbi = 'CSCvv96090, CSCvw38982';
  }
  else if (product_info.model =~ "^3[0-9]{3}")
  {
    cbi = 'CSCvv78238, CSCvw38964';
    smus['7.0(3)I7(8)'] = 'CSCvv78238';
    smus['7.0(3)I7(9)'] = 'CSCvv78238';
  }
}
if (empty_or_null(cbi)) audit(AUDIT_HOST_NOT, 'affected');

