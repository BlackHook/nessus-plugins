  );
  script_set_attribute(
    attribute:"see_also",
    value:"https://bugzilla.suse.com/show_bug.cgi?id=989997"
  );
  script_set_attribute(
    attribute:"see_also",
    value:"https://www.suse.com/security/cve/CVE-2016-1000111/"
  );
  # https://www.suse.com/support/update/announcement/2017/suse-su-20170114-1/
  script_set_attribute(
    attribute:"see_also",
